.img-wrap {
    position: relative;
    width: 100%;
    padding-top: 42.3333%;
}

.img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}


.camera{
  display:flex; 
  justify-content: center;
  margin-top: 0rpx;
}

.register{
  display:flex; 
  justify-content: center;
  margin-left: 300rpx;
  position: fixed;
  bottom: 90rpx;
}

.register image{
  width:160rpx;
  height:160rpx;
}

